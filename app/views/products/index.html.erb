<h1>All Products</h1>
<h5><%= link_to "Create New Product", new_product_path %></h5>

<% @products.each do | product | %>
  <p>
    <%= product.name %><br>
    <%= product.description %><br>
    <%= product.price_in_cents %><br>
    <%= link_to "View Product Details", product_path(product.id) %>
  </p>
<% end %>
